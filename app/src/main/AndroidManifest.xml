<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cryptoTRX.gabriel.cryptoconvert">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logov2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logov2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.cryptoTRX.gabriel.cryptoconvert.MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.cryptoTRX.gabriel.cryptoconvert.supportActivity" />
        <activity android:name="com.cryptoTRX.gabriel.cryptoconvert.termsActivity"></activity>
    </application>

</manifest>